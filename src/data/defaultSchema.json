{
  "$id": "https://example.com",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "foo": {
      "$ref": "#/$defs/foo"
    },
    "bar": { "const": null },
    "baz": {
      "type": "array",
      "prefixItems": [
        { "type": "string" },
        true,
        { "$ref": "#/$defs/bar" }
      ]
    }
  },
  "$defs": {
    "bool": false,
    "foo": {
      "type": "object",
      "properties": {
        "a": { "type": "boolean" }
      }
    },
    "bar": { "type": "number" }
  }
}